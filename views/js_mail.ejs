<!doctype html>
<html lang="ja">
	<head>
		<style>
			table {
				border-collapse: collapse;
			}

			th,
			td {
				padding: 0.5em;
				border: 1px solid;
			}

			th {
				background: #eee;
				text-align: left;
			}

			dt {
				font-weight: bold;
			}

			dd {
				margin-left: 1em;
			}
		</style>
	</head>
	<body>
		<table>
			<tbody>
				<tr>
					<th>ページ URL</th>
					<td>
						<a href="<%= location %>"><%= location %></a>
					</td>
				</tr>
				<tr>
					<th>ファイル名</th>
					<td>
						<a href="<%= filename %>"><%= filename %></a>
					</td>
				</tr>
				<tr>
					<th>行数</th>
					<td><%= lineno %></td>
				</tr>
				<tr>
					<th>列数</th>
					<td><%= colno %></td>
				</tr>
				<tr>
					<th>メッセージ</th>
					<td><%= message %></td>
				</tr>
			</tbody>
		</table>

		<dl>
			<dt>ユーザーエージェント</dt>
			<dd><%= ua %></dd>
		</dl>
	</body>
</html>
